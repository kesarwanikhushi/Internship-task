# Copy to .env and fill with real values
# MongoDB connection string
MONGODB_URI=mongodb+srv://<user>:<password>@cluster0.mongodb.net/internship-task?retryWrites=true&w=majority

# JWT secret for signing tokens (keep this secret)
JWT_SECRET=your_jwt_secret_here
# JWT token expiry (e.g. 30d, 1h)
JWT_EXPIRE=30d

# Server port
PORT=5000
